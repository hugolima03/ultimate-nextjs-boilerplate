{
  "name": "ultimate-nextjs-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --max-warnings=0",
    "test": "jest --watch",
    "test:ci": "jest --coverage --passWithNoTests",
    "generate": "yarn plop --plopfile generators/plopfile.js",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix",
      "yarn test:ci"
    ]
  },
  "dependencies": {
    "@types/node": "18.15.10",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "eslint": "8.36.0",
    "eslint-config-next": "13.2.4",
    "next": "13.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "^5.3.9",
    "typescript": "5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@storybook/addon-actions": "^7.0.0-rc.7",
    "@storybook/addon-essentials": "^7.0.0-rc.7",
    "@storybook/addon-interactions": "^7.0.0-rc.7",
    "@storybook/addon-links": "^7.0.0-rc.8",
    "@storybook/addon-mdx-gfm": "^7.0.0-rc.7",
    "@storybook/nextjs": "^7.0.0-rc.7",
    "@storybook/react": "^7.0.0-rc.7",
    "@storybook/testing-library": "^0.0.14--canary.30.5f44d14.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/styled-components": "^5.1.26",
    "babel-loader": "^9.1.2",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.11",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.0",
    "plop": "^3.1.2",
    "prettier": "^2.8.7",
    "storybook": "^7.0.0-rc.7"
  }
}
